<!--block droneInput-->
    <!--if inputLabel-->
        <label for='<!--attributes.id-->' id='<!--attributes.id-->_label'>
            <!--if2 mandatoryMarker-->
                <span class='mandatoryMarker'><!--mandatoryMarker--></span>
            <!--/if2-->
            <!--inputLabel-->:
        </label>
    <!--/if-->
    <!--if inputError-->
        <span class='inputError'><span><!--"Error"|translate(true)-->: </span><!--inputError--></span>
    <!--/if-->
    <textarea rows='5' cols='9' <!--for key,item in attributes--> <!--key-->='<!--item-->'<!--/for-->><!--inputValue--></textarea>
<!--/block-->
