<!--if inputLabel-->
    <label for='<!--attributes.id-->' id='<!--attributes.id-->_label'>
        <!--if2 mandatoryMarker-->
            <span class='mandatoryMarker'><!--mandatoryMarker--></span>
        <!--/if2-->
        <!--inputLabel-->:
    </label>
<!--/if-->
<!--if inputError-->
    <span class='inputError'><span><!--"Error"|translate-->: </span><!--inputError--></span>
<!--/if-->
<select <!--for key,item in attributes--> <!--key-->='<!--item-->'<!--/for-->>
    <!--for item in values-->
        <option value='<!--item.key-->'<!--if item.selected--> selected='selected'<!--/if-->><!--item.value--></option>
    <!--/for-->
</select>
