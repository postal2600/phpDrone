{%block droneInput%}
    <img id='captchaImage' src='phpDrone/Captcha.php?id={%captchaId%}' alt='Captcha' onclick='regenerate("{%captchaId%}")'/>
    {%inputError%}
    <span id="captchaNote"><b>Note:</b>&nbsp;Click image to regenerate</span>
    <script type='text/javascript' src='phpDrone/res/scripts/captcha.js'></script>
    <input type='hidden' name='captchaId' value='{%captchaId%}'/>
{%block droneInput%}
