{%block droneInput%}
    <label for='{%inputName%}'>{%inputLabel%}</label>
    {%if inputError%}
        <div class='error'><span>Error: </span>{%inputError%}</div>
    {%end-if%}
    <input class='textInput' type='password' id='{%inputName%}' name='{%inputName%}' value='{%inputValue%}' />
    {%for item in bunch%}
        <br />### {%item%}
    {%end-for%}
{%end-block%}
